{
  "name": "ularanigu/fusion",
  "description": "A PSR-11 compliant dependency injector.",
  "keywords": [
    "psr-11-compliant",
    "dependency-injector",
    "secure-by-default",
    "container-management",
    "easy-to-use",
    "php71-component",
    "php72-component"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/container": "^1",
    "ext-json": "*"
  },
  "authors": [
    {
      "name": "Nicholas English",
      "email": "nenglish6657@gmail.com",
      "homepage": "https://github.com/ularanigu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ularanigu\\Fusion\\": "./src"
    },
    "files": [
      "./boot/boot.php"
    ]
  },
  "autoload-dev": {
    "files": [
      "./tests/before_tests.php"
    ]
  }
  "require-dev": {
    "phpunit/phpunit": "^7",
    "vimeo/psalm": "^1"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "prefer-stable": true
}
